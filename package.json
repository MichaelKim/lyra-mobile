{
  "name": "lyra-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ports": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:8097 tcp:8097",
    "flipper": "adb shell am start -n com.lyra/com.facebook.flipper.android.diagnostics.FlipperDiagnosticActivity",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "debug": "cd android && ./gradlew assembleDebug",
    "release": "cd android && ./gradlew assembleRelease",
    "clean": "react-native-clean-project --keep-node_modules",
    "test": "jest",
    "lint": "eslint . && cd android && ./gradlew lint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/checkbox": "^0.5.7",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/slider": "^3.0.3",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@reduxjs/toolkit": "^1.5.1",
    "crypto-js": "^3.3.0",
    "immer": "^9.0.1",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-music-files": "^2.1.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.7.0",
    "react-native-music-control": "^1.3.0",
    "react-native-permissions": "^3.0.1",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.0.0",
    "react-native-svg": "^12.1.0",
    "react-native-video": "^5.1.1",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/runtime": "^7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/crypto-js": "^3.1.47",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.64.2",
    "@types/react-native-video": "^5.0.4",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-native-bundle-visualizer": "^2.2.1",
    "react-native-clean-project": "^3.6.3",
    "react-native-flipper": "^0.83.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "redux-flipper": "^1.4.2",
    "typescript": "^4.2.3"
